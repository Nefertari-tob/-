.container{
  background-color: #E1F2FF;
}
.above{
  width: 100%;
  height: 55%;
  z-index: 1;
}
.head{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;

}
.body{
  display: flex;
  flex-direction:row;
  align-items:center;
  justify-content: center;
  margin: 30rpx;
}
.icon-a{
  height: 30px;
  width: 38px;
  margin: 0 9pt 0 0;
}
.title{
  height: 22px;
  width: 208px;
  font-size: 16px;
  color: #128AD4;
  font-family: PingFang SC;
  align-content: center;
  display: flex;
  justify-content: space-around;
}
.index{
  width: 90%;
  height: 900rpx;
  background-color: white;
}
.top{
  width: 74px;
  height: 24px;
  line-height: 24px;
  background-color: #E1F2FF;
  border-radius: 14px;
  text-align: center;
  margin: 40rpx 0 0 30rpx;
}
.text-a{
  color: #333333;
  font-family: PingFang SC;
  font-size: 12px;
  margin: 3px;
}
.indexed{
  width: 100%;
  height: 490rpx;
  display: flex;
  align-items:center;
  justify-content:center;
}
.choose{
  height: 184px;
  width: 131px;
}
.picture{
    height: 152px;
    width:100px;
    margin: 0rpx  auto; 
    font-size: 12px; 
    display: flex;
    text-align: center;
    align-items: flex-end; 
    position: relative;
}
.img-text{
  width: 100%;
  font-size: 12px;
  font-family: PingFang SC;
  opacity: 0.3;
  position: absolute;
  text-align: center;
}
.but{
  padding:0%;
 
  border-style: dashed;
}
.inp{
  width: 100%;
  height: 100%;
}
.img {
  width: 100%;
  height: 100%;
}

.version{
  width: 100%;
  height: 50%;
  z-index: 2;
  background-color: #FFFFFFFF;
  border-radius: 14px 14px 0 0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.one{
  width: 74px;
  height: 24px;
  background-color: #E1F2FF;
  border-radius: 14px;
  text-align: center;
  margin: 40rpx 0 0 30rpx;
}
.text-b{
  color: #333333;
  font-family: PingFang SC;
  font-size: 12px;
  margin: 3px;
}
.two{
  width: 100%;
  height: 70%;
  display: flex;
  justify-content: center;
  margin: 25rpx 0 25rpx 0;
}
.text-c{
  width: 80%;
  color: #333333;
  font-family: PingFang SC;
  font-size: 12px;
}
.three{
  width: 100%;
  display: flex;
  justify-content: center;
  margin: 0 0 50rpx 0;
}
.button{
  width: 75%;
  display: flex;
  justify-content: center;
}
.button-a{
  line-height: 6px;
  font-family: PingFang SC;
  font-size: 12px;
  color: white;
  background-color: #128AD4;
  border-radius: 8px;
}
.button-a:active{
	transform: translateY(-1px);
	box-shadow: 0 5px 10px rgba(0,0,0,0.2);
}
.button-b{
  line-height: 6px;
  font-family: PingFang SC;
  font-size: 12px;
  color: white;
  background-color: #128AD4;
  border-radius: 8px;
}
.button-b:active{
	transform: translateY(-1px);
	box-shadow: 0 5px 10px rgba(0,0,0,0.2);
}